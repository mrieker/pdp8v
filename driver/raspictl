#!/bin/bash
dz=$0
dd=`dirname $dz`
$dd/loadmod.sh
. $dd/iowsns.si
$dd/iowowner.sh
if [ "$scriptini" == "" ]
then
    export scriptini=$dd/scriptini.tcl
fi
dbg=''
if [ "$1" == "-gdb" ]
then
    dbg='gdb --args'
    shift
elif [ "$1" == "-valgrind" ]
then
    dbg='valgrind'
    shift
fi
exec $dbg $dz.`uname -m` "$@"
